# Server port
PORT=4000

# JWT secret for authentication
JWT_SECRET=your_jwt_secret_here

# Google OAuth Client ID for Google SSO
GOOGLE_CLIENT_ID=your_google_client_id_here

# Prisma database URL (Postgres example)
DATABASE_URL=postgresql://user:password@localhost:5432/gashy_db?schema=public

# Redis URL
REDIS_URL=redis://localhost:6379

# Payment gateway API key (mock or real)
PAYMENT_API_KEY=your_payment_api_key_here
